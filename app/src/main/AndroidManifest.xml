<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cp4">
     <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@drawable/ic_ex"
        android:label="cp4"
        android:roundIcon="@drawable/ic_ex"
        android:supportsRtl="true"
        android:theme="@style/Theme.Cp3">
        <activity android:name="com.example.cp4.dtrains"></activity>
        <activity android:name="com.example.cp4.cp" />
        <activity android:name="com.example.cp4.Lister" />
        <activity
            android:name="com.example.cp4.Barc"
            android:theme="@android:style/Theme.Black.NoTitleBar" />
        <activity android:name="com.example.cp4.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>